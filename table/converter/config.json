{
    "keyword": {
        "key": "*",
        "nullable": "?",
        "relation": "$",
        "percentage": "%",
        "unknown": "~"
    },
    "usage": {
        "server": "go",
        "client": "cs"
    },
    "primitives": 
    [
        "int", "long", "double", "float", "string", "bool", "DateTime", "TimeSpan", "Dsl", "Point"
    ],
    "regex":
    {
        "group": "^\\((?P<type>.+)\\)$",
        "list": "^List<(?P<type>.+)>$",
        "array": "^\\[(?P<type>.*)\\]$",
        "map": "^\\s*{\\s*(?P<key>[.\\S]+)\\s*:\\s*(?P<value>[.\\S]+)\\s*}\\s*$",
        "dict": "^Dictionary<(?P<key>.+),\\s*(?P<value>.+)>$",
        "const": "Const:(?P<table>[a-zA-Z]+):(?P<name>[a-zA-Z]+)",
        "dsl": "^(?P<header>\\w+)\\((?P<parameters>.*)\\)$",
        "point": "^\\((?P<x>\\d),\\s?(?P<y>\\d)\\)$"
    },
    "file":
    {
        "enum":
        {
            "prefix": "enum.",
            "suffix": null
        },
        "const":
        {
            "prefix": "const.",
            "suffix": null
        }
    },
    "dsl": 
    {
    }
}
