{
    "keyword": {
        "key": "*",
        "nullable": "?",
        "relation": "$",
        "percentage": "%",
        "ordered": "~",
        "strongRef": "!"
    },
    "primitive": [
        "int",
        "long",
        "double",
        "float",
        "string",
        "bool",
        "DateTime",
        "TimeSpan",
        "Dsl",
        "DateRange"
    ],
    "regex": {
        "group": "^\\((?P<type>.+)\\)$",
        "list": "^List<(?P<type>.+)>$",
        "map": "^\\s*{\\s*(?P<key>[.\\S]+)\\s*:\\s*(?P<value>[.\\S]+)\\s*}\\s*$",
        "dict": "^Dictionary<(?P<key>.+),\\s*(?P<value>.+)>$",
        "array": "^\\[(?P<type>.*)\\]$",
        "const": "Const:(?P<table>[a-zA-Z_]+):(?P<name>[a-zA-Z_]+)",
        "dsl": "^(?P<header>\\w+)\\((?P<parameters>.*)\\)$"
    },
    "file":
    {
        "enum":
        {
            "prefix": "enum.",
            "suffix": null
        },
        "const":
        {
            "prefix": "const.",
            "suffix": null
        }
    },
    "dsl": 
    {
    }
}
